v = mfcc_cyy(x, fs); 
% Compute MFCC's 
distmin = inf; 
k1 = 0;  
for l = 1:length(code) 
% each trained codebook, compute distortion 
d = disteu(v, code{l}); 
dist = sum(min(d,[],2)) / size(d,1); 
if dist < distmin 
distmin = dist; 
k1 = l; 
end 
end  
msg = sprintf('Speaker %d matches with speaker %d', k, k1); 
disp(msg); 

